{
  "name": "expo-flutter",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "flutter:build:android": "cd flutter_module && flutter build aar && cd -",
    "flutter:build:ios": "sh scripts/flutter_build_ios.sh",
    "flutter:build:web": "cd flutter_module && flutter build web --base-href $PUBLIC_URL/flutter/ --output=../public/flutter && cd -",
    "build:plugin": "cd modules/expo-flutter-view && expo-module build plugin",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild:web": "npm run flutter:build:web",
    "build:web": "expo export -p web",
    "gh:prepare:web": "bash scripts/prepare_web.sh"
  },
  "dependencies": {
    "@expo/html-elements": "^0.5.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "expo": "~49.0.5",
    "expo-build-properties": "~0.8.3",
    "expo-splash-screen": "~0.20.4",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.3",
    "react-native-paper": "^5.9.1",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-svg": "13.9.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.0.14",
    "expo-module-scripts": "^3.0.11",
    "typescript": "^5.1.3"
  },
  "private": true
}
